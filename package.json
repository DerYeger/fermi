{
	"name": "fermi",
	"type": "module",
	"version": "1.1.0",
	"private": true,
	"packageManager": "bun@1.2.22",
	"description": "Fermentation Tracking App",
	"license": "MIT",
	"engines": {
		"node": ">=24"
	},
	"scripts": {
		"bump": "bumpp",
		"ci": "bun run turbo build lint test typecheck",
		"dev": "tauri dev",
		"dev:nuxt": "nuxt dev",
		"build": "tauri build",
		"build:nuxt": "nuxt generate",
		"icon": "tauri icon ./resources/icon.svg && cp ./src-tauri/icons/icon.png ./public/icon.png && echo 'WARNING: Make sure to manually re-export the MacOS icon'",
		"lint": "eslint . --fix",
		"postinstall": "nuxt prepare",
		"preinstall": "npx only-allow bun",
		"tauri": "tauri",
		"test": "vitest --coverage",
		"turbo": "CI=true FORCE_COLOR=1 turbo",
		"typecheck": "nuxt typecheck"
	},
	"dependencies": {
		"@iconify-json/hugeicons": "1.2.21",
